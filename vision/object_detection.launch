<launch>
    <param name="names" textfile="$(find vision)/scripts/class-names.txt"/>
    <!-- <arg name="model_path" default="$(find vision)/imports/vision/MSCV2/yolov3m-3-416.pt"/> -->
    <arg name="model_path" default="$(find vision)/imports/vision/models/7_16_2023/best_small.pt"/>
    <arg name="model_config_path" default="$(find vision)/imports/vision/MSCV2/cfg/yolov3-mid-416.cfg"/>
    <arg name="camera_name" default="left_camera"/>
    <param name="model_path" value="$(arg model_path)"/>
    <param name="model_config_path" value="$(arg model_config_path)"/>
    <param name="camera_name" value="$(arg camera_name)"/>
    <!-- <param name="test_image_path" value="$(find vision)/imports/vision/IMG_3123.jpg"/> -->
    <param name="test_image_path" value="$(find vision)/imports/vision/toy_gate.jpg"/>
    <node name="camera_test" pkg="vision" type="CameraTest.py" output="screen"/>
</launch>